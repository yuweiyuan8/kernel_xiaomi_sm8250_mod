# Platform
CONFIG_MACH_XIAOMI_DAGU=y

# Touchscreen
CONFIG_TOUCHSCREEN_NT36523_HOSTDL_SPI=y
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y
CONFIG_TOUCHSCREEN_NEW_PEN_CONNECT_STRATEGY=y
CONFIG_TOUCHSCREEN_HIMAX_CHIPSET=y
CONFIG_TOUCHSCREEN_HIMAX_COMMON=y
CONFIG_TOUCHSCREEN_HIMAX_INCELL=y
CONFIG_TOUCHSCREEN_HIMAX_IC_HX83121=y
CONFIG_TOUCHSCREEN_HIMAX_DEBUG=y
CONFIG_TOUCHSCREEN_HIMAX_INSPECT=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Power
CONFIG_QPNP_FG_GEN4=n
CONFIG_QPNP_SMB5=n
CONFIG_QPNP_SMB5_DAGU=y
CONFIG_IDT_P9418=y
CONFIG_DUAL_FUEL_GAUGE_BQ27Z561_DAGU=y
CONFIG_BQ2597X_CHARGE_PUMP_DAGU=y
CONFIG_DUAL_BQ2597X=y

# Enable Dual KTZ8866 backlight driver
CONFIG_BACKLIGHT_DUALKTZ8866=y

# Disable ESOC
# CONFIG_ESOC is not set

# Ultrasound Proximity
CONFIG_US_PROXIMITY=y

# Enable NANOSIC Keyboard
CONFIG_KEYBOARD_NANO_8030=y

# Vibrator
CONFIG_INPUT_AW8697_HAPTIC=y
CONFIG_INPUT_AW8697_HAPTIC_DAGU=y

# Camera
CONFIG_TECHPACK_CAMERA_XIAOMI_TABLET=y

# USB PD
# CONFIG_USB_PD_POLICY is not set
CONFIG_USB_PD_POLICY_DAGU=y
CONFIG_PS5169=y

# Airoha GPS
CONFIG_AIROHA_GPS=y

# Enable modified MSM Serial driver
# CONFIG_SERIAL_MSM_GENI is not set
CONFIG_SERIAL_MSM_GENI_ELISH=y
