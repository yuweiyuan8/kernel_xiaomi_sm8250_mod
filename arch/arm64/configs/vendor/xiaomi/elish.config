# Platform
CONFIG_MACH_XIAOMI_ELISH=y

# Touchscreen
CONFIG_TOUCHSCREEN_NVT_DEBUG_FS=y
CONFIG_TOUCHSCREEN_ST=y
CONFIG_TOUCHSCREEN_ST_I2C="st"
CONFIG_TOUCHSCREEN_NT36523_HOSTDL_SPI=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_I2C=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_CORE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_TOUCH=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_DEVICE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_DIAGNOSTICS=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_GOODIX=y
CONFIG_FINGERPRINT_FPC=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Power
CONFIG_IDT_P9418=y
CONFIG_DUAL_FUEL_GAUGE_BQ27Z561=y
CONFIG_BQ2597X_CHARGE_PUMP=y
CONFIG_CHARGER_LN8000=y
CONFIG_QPNP_FG_GEN4=n
CONFIG_HL6111R=y
CONFIG_DUAL_BQ2597X=y

# Ultrasound Proximity
CONFIG_US_PROXIMITY=y

# Vibrator
CONFIG_INPUT_AW8697_HAPTIC=y

# Camera
CONFIG_TECHPACK_CAMERA_XIAOMI_TABLET=y

# Backlight
CONFIG_BACKLIGHT_KTZ8866=y

# Enable modified MSM Serial driver
# CONFIG_SERIAL_MSM_GENI is not set
CONFIG_SERIAL_MSM_GENI_ELISH=y

# Disable Multicast IGMP snooping
# CONFIG_BRIDGE_IGMP_SNOOPING is not set

# Disable ESOC
# CONFIG_ESOC is not set

# Enable Xiaomi keyboard
CONFIG_XIAOMI_KEYBOARD=y

# Retry i2c operations on failure
CONFIG_I2C_RETRY=y

CONFIG_FB_ARMCLCD=y
CONFIG_FB_CFB_FILLRECT=y
CONFIG_FB_CFB_COPYAREA=y
CONFIG_FB_CFB_IMAGEBLIT=y
CONFIG_FB_MODE_HELPERS=y
CONFIG_VIDEOMODE_HELPERS=y

# Disable US_PROXIMITY for ELISH
CONFIG_US_PROXIMITY=n